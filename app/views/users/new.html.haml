- breadcrumb :users_new
- content_for(:title, '社員登録')

= form_with model: @user, url: @user.new_record? ? users_path : user_path(@user), local: true, html: { class: 'needs-validation', novalidate: true } do |f|
  = render "users/user_form", { f: f, user: @user }
  = f.submit "登録", class: 'btn btn-primary'
